{
  "name": "imfo",
  "version": "0.0.0",
  "description": "imfo",
  "homepage": "git@git.virtualforce.io:umer.asif/imfo_api.git",
  "author": {
    "name": "Faizan Ahmad",
    "email": "faizan.ahmad@virtualforce.io"
  },
  "main": "build/app/server.js",
  "keywords": [
    "Node",
    "Express",
    "Typescript",
    "Sequelize",
    "MySQL"
  ],
  "scripts": {
    "build": "gulp build",
    "doc": "gulp generate:doc",
    "start": "gulp watch",
    "dev": "gulp watch",
    "run:test": "cross-env NODE_ENV=test gulp test",
    "tslint": "gulp tslint",
    "precommit": "lint-staged",
    "prettify": "tsfmt -r --baseDir ./src",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:rollback": "./node_modules/.bin/sequelize db:migrate:undo",
    "generate:migration": "./node_modules/.bin/sequelize migration:create"
  },
  "lint-staged": {
    "app/**/*.ts": [
      "prettify",
      "git add"
    ]
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "~1.17.0",
    "cors": "~2.8.1",
    "cross-env": "~3.2.3",
    "crypto": "^1.0.1",
    "dotenv": "^6.1.0",
    "express": "~4.15.0",
    "express-boom": "~2.0.0",
    "express-subdomain": "^1.0.5",
    "express-validator": "^3.1.3",
    "glob": "~7.1.1",
    "gulp": "~3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-istanbul": "~1.1.1",
    "gulp-json-refs": "~0.1.1",
    "gulp-mocha": "~3.0.1",
    "gulp-nodemon": "~2.2.1",
    "gulp-plumber": "~1.1.0",
    "gulp-sourcemaps": "~2.4.0",
    "gulp-tslint": "~7.0.1",
    "gulp-typedoc": "^2.2.1",
    "gulp-typescript": "~3.1.4",
    "husky": "^0.13.2",
    "jsonwebtoken": "^8.4.0",
    "lint-staged": "^3.4.0",
    "lodash": "^4.17.11",
    "morgan": "~1.8.1",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "nodemailer": "^4.7.0",
    "nodemailer-express-handlebars": "^3.0.0",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "remap-istanbul": "~0.8.4",
    "rimraf": "~2.5.4",
    "run-sequence": "~1.2.2",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^2.7.0",
    "swagger-jsdoc": "^3.2.6",
    "swagger-ui-express": "^4.0.1",
    "tslint": "~4.5.1",
    "typedoc": "~0.5.7",
    "typescript": "~2.2.1",
    "typescript-formatter": "^5.1.1",
    "uuid": "~3.0.1",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "~0.0.33",
    "@types/chai": "~3.4.35",
    "@types/express": "~4.0.35",
    "@types/express-validator": "~2.20.33",
    "@types/mocha": "~2.2.39",
    "@types/morgan": "~1.7.32",
    "@types/sequelize": "~3.4.40",
    "@types/winston": "~2.2.0",
    "chai": "~3.5.0",
    "extendify": "~1.0.0"
  },
  "engines": {
    "node": ">=8.11.1"
  },
